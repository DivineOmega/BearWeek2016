pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
scene = {}

pl = {}
hunters = {}

function _init()
 palt(0, false)
 palt(13, true)
 init_game_scene()
end

function _update()
 scene.update()
end

function _draw()
 scene.draw()
end

function init_game_scene()

 pl = create_player()

 scene.update = game_update
 scene.draw = game_draw
end

function game_update()
 pl.update()
end

function game_draw()
 cls()
 rectfill(0, 0, 128, 128, 11)
 pl.draw()
end

function create_player()
 local pl = {}
 pl.width = 14
 pl.height = 20
 pl.x = 64 - (pl.width*0.5)
 pl.y = 64 - (pl.height*0.5)
 pl.spd = 2
 pl.moving = false
 pl.moving_anim_step = 0
 pl.moving_timer = 0
 
 pl.draw = function()
  spr(0, pl.x, pl.y, 2, 3)
  
  if (pl.moving) then
   if (pl.moving_anim_step == 0) then
    spr(2, pl.x, pl.y+16, 2, 1)
   else
    spr(18, pl.x, pl.y+16, 2, 1)
   end
  end
 end
 
 pl.update = function()
 
  pl.moving = false
 
  if (btn(0)) then
   pl.moving = true
   pl.x -= pl.spd
  end
  if (btn(1)) then
   pl.moving = true
   pl.x += pl.spd
  end
  if (btn(2)) then
   pl.moving = true
   pl.y -= pl.spd
  end
  if (btn(3)) then
   pl.moving = true
   pl.y += pl.spd
  end
  
  if (pl.x < 0) then
   pl.x = 0
   pl.moving = false
  elseif (pl.x+pl.width>128) then
   pl.x = 128-pl.width
   pl.moving = false
  end
  
  if (pl.y < 0) then
   pl.y = 0
   pl.moving = false
  elseif (pl.y+pl.height>128) then
   pl.y = 128-pl.height
   pl.moving = false
  end
  
  if (pl.moving) then
   pl.moving_timer += 1
   if (pl.moving_timer >= 5) then
    pl.moving_timer = 0
    if (pl.moving_anim_step == 0) then
     pl.moving_anim_step = 1
    else
     pl.moving_anim_step = 0
    end
   end
  end
  
 end
 
 return pl
end

function create_hunter()
 local h = {}
 h.width = 14
 h.height = 20
 h.x = 0
 h.y = 0
 h.spd = 0.25
 
 h.rand_pos = function()
  if (rnd(100)<0.5) then
   h.x = 0 - h.width
  else
   h.x = 128
  end
  h.y = rnd(128-h.height)
 end
 
 h.update = function()
  
 end
 
 h.draw = function()
  spr(4, h.x, h.y, 2, 2)
 end
 
 return h
end
